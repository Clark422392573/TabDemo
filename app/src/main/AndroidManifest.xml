<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clark.tabdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/settings_android"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".personaldata.PersonalDataActivity" />
        <activity
            android:name=".zoomheader.MainParticularsActivity"
            android:theme="@style/translucent" />
        <activity android:name=".im.IMActivity" />
        <activity android:name=".show.ShowActivity"></activity>
    </application>

</manifest>